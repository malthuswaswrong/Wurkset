<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <FileVersion>0.2</FileVersion>
    <AssemblyVersion>0.2</AssemblyVersion>
    <Title>Wurkset</Title>
    <Description>A simple slow file system based object repository</Description>
    <PackageProjectUrl>https://github.com/malthuswaswrong/Wurkset</PackageProjectUrl>
    <RepositoryUrl>https://github.com/malthuswaswrong/Wurkset</RepositoryUrl>
    <PackageTags>worket;repository;file;filesystem;directory</PackageTags>
    <PackageReleaseNotes>Initial release</PackageReleaseNotes>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Authors>$(AssemblyName)</Authors>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options" Version="6.0.0" />
    <PackageReference Include="NUlid" Version="1.7.0" />
  </ItemGroup>

</Project>
